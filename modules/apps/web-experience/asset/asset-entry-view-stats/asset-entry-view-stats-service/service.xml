<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder auto-namespace-tables="false" package-path="com.liferay.asset.entry.view.stats">
	<namespace>AssetEntryViewStats</namespace>
	<entity local-service="true" name="AssetEntryViewStats" remote-service="true">

		<!-- PK fields -->

		<column accessor="true" name="statsId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />

		<!-- Other fields -->

		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />
		<column name="assetEntryId" type="long" />

		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="C_C_GtD" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column comparator="&gt;" name="createDate" />
		</finder>
		<finder name="C_C_LtD" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column comparator="&lt;" name="createDate" />
		</finder>
	</entity>
</service-builder>